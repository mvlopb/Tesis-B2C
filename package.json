{
  "name": "full-stack-template-2",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generate-docs": "jsdoc -d docs --configure jsDocConfig.json",
    "test": "turbo test",
    "populate:stock": "ts-node apps/api/src/scripts/populateStock.ts",
    "populate:orders": "ts-node apps/api/src/scripts/populateOrders.ts",
    "populate:tickets": "ts-node apps/api/src/scripts/populateTickets.ts",
    "populate:payments": "ts-node apps/api/src/scripts/populatePayments.ts",
    "restock": "ts-node apps/api/src/scripts/restock.ts",
    "reset:quantity": "ts-node apps/api/src/scripts/resetSoldQty.ts",
    "populate:address": "ts-node apps/api/src/scripts/populateAddress.ts",
    "populate:clients": "ts-node apps/api/src/scripts/populateClients.ts",
    "populate:employees": "ts-node apps/api/src/scripts/populateEmployees.ts",
    "change:categories": "ts-node apps/api/src/scripts/changeCategories.ts",
    "addProductData": "ts-node apps/api/src/scripts/addProductData.ts",
    "insert:orders": "ts-node apps/api/src/scripts/insertOrders.ts"









  },
  "devDependencies": {
    "@avila-tek/eslint-config": "*",
    "@avila-tek/typescript-config": "*",
    "@faker-js/faker": "^9.0.3",
    "prettier": "^3.2.4",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@9.6.7",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
